{
  "name": "@renex/renex",
  "version": "0.4.7",
  "description": "The official RenEx Software Development Kit in typescript.",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/republicprotocol/renex-sdk-ts.git"
  },
  "keywords": [
    "renex",
    "republic",
    "republicprotocol",
    "darkpool",
    "sdk"
  ],
  "author": "Republic Protocol",
  "license": "GPL-3.0",
  "bugs": {
    "url": "https://github.com/republicprotocol/renex-sdk-ts/issues"
  },
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "lint:unused-exports": "ts-unused-exports ./tsconfig.json",
    "lint:outdated-imports": "ncu",
    "lint:ts": "tslint --project ./ -t stylish --quiet",
    "lint:extra": "$npm_execpath run --silent lint:unused-exports; $npm_execpath run --silent lint:outdated-imports",
    "lint:all": "$npm_execpath run --silent lint:ts && $npm_execpath run --silent lint:extra",
    "lint": "$npm_execpath run --silent lint:ts",
    "watch": "cross-env NODE_ENV=development webpack --watch --mode development --display-error-details --progress --color",
    "build:dev": "cross-env NODE_ENV=development webpack --mode development --display-error-details --progress --color",
    "build:prod": "$npm_execpath run clean && cross-env NODE_ENV=production webpack --mode production --progress --color",
    "test": "./node_modules/ts-mocha/bin/ts-mocha --timeout 180000 --paths -p ./tsconfig.json ./test/*.spec.ts ./test/**/*.spec.ts",
    "clean": "rm -rf dist",
    "prepublishOnly": "$npm_execpath run lint",
    "prepare": "$npm_execpath run build:dev"
  },
  "devDependencies": {
    "@babel/core": "^7.4.3",
    "@babel/plugin-proposal-object-rest-spread": "^7.4.3",
    "@types/chai": "^4.1.7",
    "@types/chai-as-promised": "7.1.0",
    "@types/expect": "^1.20.4",
    "@types/mocha": "^5.2.6",
    "awesome-typescript-loader": "5.2.1",
    "babel-loader": "8.0.5",
    "babel-plugin-transform-es2015-modules-simple-commonjs": "^0.3.0",
    "babel-polyfill": "6.26.0",
    "chai": "^4.2.0",
    "chai-as-promised": "^7.1.1",
    "cross-env": "^5.2.0",
    "dotenv": "^7.0.0",
    "mocha": "^6.1.3",
    "npm-check-updates": "^3.1.8",
    "ts-loader": "^5.3.3",
    "ts-mocha": "^6.0.0",
    "ts-unused-exports": "^2.0.11",
    "tslint": "^5.16.0",
    "tslint-microsoft-contrib": "^6.1.1",
    "tslint-react": "^4.0.0",
    "typedoc": "0.14.2",
    "typedoc-webpack-plugin": "1.1.4",
    "typescript": "^3.4.3",
    "webpack": "^4.30.0",
    "webpack-cli": "^3.3.0",
    "webpack-node-externals": "1.7.2"
  },
  "dependencies": {
    "@types/bn.js": "^4.11.5",
    "@types/bs58": "4.0.0",
    "@types/node-rsa": "1.0.0",
    "axios": "0.18.0",
    "bignumber.js": "^8.1.1",
    "bn.js": "4.11.8",
    "bs58": "4.0.1",
    "immutable": "^4.0.0-rc.12",
    "node-rsa": "^1.0.5",
    "web3": "^1.0.0-beta.52",
    "web3-utils": "^1.0.0-beta.52"
  }
}
